{
  "name": "sober",
  "version": "0.6.7",
  "description": "Sober is a library of UI components designed with reference to Material You.",
  "type": "module",
  "scripts": {
    "dev": "node dev/main.js",
    "build": "npx tsc && npx rollup --config dev/rollup.config.js",
    "publish": "npm publish"
  },
  "keywords": [
    "sober",
    "material you",
    "material design",
    "web components"
  ],
  "exports": {
    ".": "./dist/main.js",
    "./style/*": "./style/*",
    "./*": "./dist/*.js"
  },
  "author": "apprat",
  "repository": "apprat/sober",
  "homepage": "https://soberjs.com",
  "license": "MIT",
  "main": "./dist/main.js",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "rollup": "^4.24.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^5.6.3",
    "csstype": "^3.1.3"
  },
  "dependencies": {}
}